#include <stdio.h>

int main() {
    int a[100], b[100];
    int n, i;

    // Read number of elements
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Read elements of first array
    printf("Enter %d elements of first array:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    // Copy elements to second array
    for(i = 0; i < n; i++) {
        b[i] = a[i];
    }

    // Display both arrays
    printf("\nFirst Array:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }

    printf("\nSecond Array (Copied):\n");
    for(i = 0; i < n; i++) {
        printf("%d ", b[i]);
    }

    return 0;
}
