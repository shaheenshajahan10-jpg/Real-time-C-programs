#include <stdio.h>

int main() {
    int num, i;

    // Ask user for the number
    printf("Enter a number to display its multiplication table: ");
    scanf("%d", &num);

    // Display multiplication table from 1 to 10
    for(i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", num, i, num * i);
    }

    return 0;
}
