#include <stdio.h>

int main() {
    int i, n;
    float expense[50], total = 0;

    printf("Enter number of expense categories: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter expense %d amount: ", i + 1);
        scanf("%f", &expense[i]);
        total = total + expense[i];
    }

    printf("\nTotal Monthly Expenses = %.2f", total);

    return 0;
}
