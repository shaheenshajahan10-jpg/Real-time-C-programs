#include <stdio.h>
#include <string.h>

// Define structure
struct Login {
    char username[20];
    char password[20];
};

int main() {
    struct Login stored, entered;

    // Store credentials
    printf("Create Username: ");
    scanf("%s", stored.username);

    printf("Create Password: ");
    scanf("%s", stored.password);

    // Enter credentials for login
    printf("\nEnter Username: ");
    scanf("%s", entered.username);

    printf("Enter Password: ");
    scanf("%s", entered.password);

    // Verify credentials
    if (strcmp(stored.username, entered.username) == 0 &&
        strcmp(stored.password, entered.password) == 0) {
        printf("\nLogin Successful!");
    } else {
        printf("\nInvalid Username or Password!");
    }

    return 0;
}
